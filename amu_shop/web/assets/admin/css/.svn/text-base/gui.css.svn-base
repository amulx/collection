
.red,.red:link,.red:visited,.red:hover {
color:#FF0000;
}
.blue,.blue:link,.blue:visited,.blue:hover {
color:#0000ff;
}
div,ul,form,table {font-size:12px;margin:0;clear:both;}
li {list-style:none outside;}

body {
color: #333;
margin: 0;
font-family: Arial, Helvetica, sans-serif;
font-size: 12px;
line-height: 150%;
background:#fff;
text-align:center;
word-break:break-all;
} 
a:link{font-size: 12px;text-decoration:underline;color:#008ED0;}
a:visited {text-decoration:underline;color:#008ED0;}
a:hover {text-decoration:underline;color:#ff9900;}
a:active {text-decoration:none;color:#666;}
.left {float:left;}
.right {float:right;}

/* admin_index_left */
#TabPage1 {width:180px;margin:0 0 0 3px;}
ul.left_top {width:156px;background:#fff;text-align:left;cursor:pointer;margin:0 0 0 -40px!important;margin:0;}
ul.left_top li {float:left;}
ul.left_top li.left_top_left {padding:8px 0 3px 22px;height:20px;width:124px!important;width:146px;font-weight:900;color:#fff;background: url("admin_top_left.png") no-repeat 0 0;}
ul.left_top li.left_top_right {height:30px;width:10px;background: url("admin_top_right.png") no-repeat 0 0;}

ul.left_conten {padding:5px 0 0 14px;width:155px;text-align:left;background: url("left_conten.png") repeat-y left top;}
ul.left_conten li {line-height:1.8;list-style:none outside;}
ul.left_conten a.Selected {padding:5px 6px 2px 6px;color:#f00;border:1px #92B0DD solid;background:#E2EAF8;text-decoration:none;}
ul.left_conten li a {padding:0 7px 0 7px;}
ul.left_conten li a:hover {padding:5px 6px 2px 6px;color:#f00;border:1px #92B0DD solid;background:#E2EAF8;text-decoration:none;}

div.left_top {width:160px;height:15px;background: url("left_top.png") no-repeat left bottom;}
div.left_end {width:160px;height:15px;background: url("left_end.png") no-repeat left top;}
/* admin_index_main */
#main_body {width:100%;margin:0 auto;}

ul.main_top {width:auto!important;width:100%;height:27px;font-size:14px;font-weight:900;color:#fff;background: url("admin_top_left.png") no-repeat 0 0;}
ul.main_top li.main_top_left {padding:7px 0 3px 22px;height:20px; color:#000}
ul.main_top li.main_top_right {height:30px;width:10px;background: url("admin_top_right.png") no-repeat 0 0;}

div.main_content_rightbg {width:auto;padding:0 3px 0 0;background: url("main_content_rightbg.png") repeat-y right top;}
div.main_content_leftbg {line-height:1.8;padding:10px 12px 0 15px;background:#fff url("main_content_leftbg.png") repeat-y left top; text-align:left;}
div.main_content_leftbg a {text-decoration:underline;}
div.main_content_leftbg table {width:100%!important;width:96%;margin:0!important;margin:0 -30px 0 -30px;background:#eee;}

div.main_content_leftbg #textarea table {width:auto;margin:0;background:#fff;}
div.main_content_leftbg #textarea table td {line-height:1.0;padding:0;}


div.main_content_leftbg table tbody tr {background:#fff;}
div.main_content_leftbg table thead tr {background:#EEEEEE;}
div.main_content_leftbg .table_content tbody tr:hover {background:#D9E4FF;}
div.main_content_leftbg table td {line-height:1.8;padding:3px 10px;border:none;}
div.main_content_leftbg table td.topbg {padding:0;font-size:13px;color:#000;text-align:left;background: url("main_content_topbg.png") repeat-x left top;border:1px #1E79B9 solid;margin:-1px;}
div.main_content_leftbg table td.topbg strong {padding:0 0 0 10px;display:block;border:1px #fff solid;}
div.main_content_leftbg table td.topbg b {padding:0 0 0 10px;display:block;border:1px #fff solid;}
div.main_content_leftbg table td.topbg1 {padding:4px 10px 0 0;font-size:13px;color:#000;text-align:right;background: url("main_content_topbg.png") repeat-x left top;border:1px #999 solid;margin:-1px;}



.pages{font-size:12px; color:#000; text-align:center; zoom:1; padding:10px 0; }
.pages a{color:#075aaa;padding:3px 3px 3px 3px;}
.pages span{font-size:12px; margin:0px 2px;padding:2px 2px 2px 2px;text-align:center;border:1px solid #D1E7FF;color:#075aaa; cursor:pointer}
.pages span .prenext{text-decoration:none;}
.pages .pagesOn{color:#000; border-color:#fff}
.pages .pagesnone{color:#999; border-color:#999; cursor:default;}

ul.main_end {background: url("main_end_left.png") no-repeat 0 0;height:20px;}
ul.main_end li.main_end_right {height:20px;width:10px;background: url("main_end_right.png") no-repeat 0 0;}


div.progress1 {
	height: 20px;
	width: 400px;
	border: 1px solid #000;
	background-color: #fff;
	text-align: left;
	MARGIN-top:15px;
	margin-bottom: 5px;
}
div.progress2 {
	height: 20px;
	border: 0px none;
	background: url("main_content_topbg.png") repeat-x left top;
	MARGIN: 1px 1px 1px 1px;
	text-align: left;
}

.title {
    background-image:url(/manager/images/TxtRule.jpg);
}
.bf_btn{ width:108px; height:29px;background:url(/manager/images/bf_btn.gif);text-align:center; line-height:29px;color:#FFFFFF;border:0; cursor:pointer; font-weight:bold; }
.tk_btn{ width:108px; height:29px;background:url(/manager/images/tk_btn.gif); text-align:center; line-height:29px;color:#000000;border:0; cursor:pointer; font-weight:bold; }
/*设定按钮大小*/
input[type=submit], input[type=cancel], input[type=button]{width: 100px;height: 30px;}
/*为分页当前页指定颜色*/
.page .current{color: red}
.live_video{color: #B50000}
.taped_video{color: #009900;}
.hide-item{display: none;}
.show-item{display: block;}
.show-table-item{display: run-in;}
.gray{color: #aaaaaa;}